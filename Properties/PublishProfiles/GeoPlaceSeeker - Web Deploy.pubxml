<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <ResourceId>/subscriptions/fc476326-776c-495f-8afe-1202b9e55bcd/resourcegroups/GeoPlaceSeekerResourceGroup/providers/Microsoft.Web/sites/GeoPlaceSeeker</ResourceId>
    <ResourceGroup>GeoPlaceSeekerResourceGroup</ResourceGroup>
    <PublishProvider>AzureWebSite</PublishProvider>
