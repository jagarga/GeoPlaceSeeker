            <Source Path="DBContext" DbContext="GeoPlaceSeeker.Models.ApplicationDbContext, GeoPlaceSeeker" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DefaultConnection-Web.config Connection String" />
